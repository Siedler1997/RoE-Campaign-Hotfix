<?xml version="1.0" encoding="utf-8" standalone="no" ?><root>
    <Display classname="ED::CBuildingProps">
        <Class>ED::CDisplayBuilding</Class>
        <Model>Buildings/B_FenceGate</Model>
        <Banner>Default</Banner>
        <Anims/>
        <ConstructionSite>Buildings/B_BuildingPlot_PalisadeGate</ConstructionSite>
    </Display>
    <Logic classname="EGL::CWallGateProps">
        <Class>EGL::CWallGate</Class>
        <AccessCategory>AccessCategoryBuilding</AccessCategory>
        <MapFileDontSave>true</MapFileDontSave>
        <EntityCategories>
            <EntityCategory>Wall</EntityCategory>
            <EntityCategory>Fence</EntityCategory>
            <EntityCategory>CityWallSegment</EntityCategory>
        </EntityCategories>
        <NeedsPlayer>true</NeedsPlayer>
        <ForceNoPlayer>false</ForceNoPlayer>
        <RemovableBlocking>true</RemovableBlocking>
        <DeleteWhenBuiltOn>true</DeleteWhenBuiltOn>
        <Worker>U_WallConstructionWorker</Worker> <!-- construction only -->
        <MaxWorkers>0</MaxWorkers>                <!-- construction only -->
        <MaxHealth>5</MaxHealth>
        <Exploration>0</Exploration>
        <CanBeKnockedDown>true</CanBeKnockedDown>
        <FlattenOnBuild>false</FlattenOnBuild>
        <IsHeightLocking>true</IsHeightLocking>
        <CanBeBuildOnOuterRim>true</CanBeBuildOnOuterRim>
        <FirstTurretType>B_FenceTurret</FirstTurretType>
        <SecondTurretType>B_FenceTurret</SecondTurretType>
        <MaxHeightDelta>300</MaxHeightDelta>
        <Lengths>
            <Length>500</Length>
        </Lengths>
        <WallHeight>100</WallHeight>
        <IsMountable>false</IsMountable>
        <OpenGateAnimID>B_FenceGate_OpenGate_600</OpenGateAnimID>
        <OpenGateSubAnimIdx>0</OpenGateSubAnimIdx>
        <CloseGateAnimID>B_FenceGate_CloseGate_600</CloseGateAnimID>
        <CloseGateSubAnimIdx>0</CloseGateSubAnimIdx>
        <AlarmRepairBuildingTL>TL_WORKER_REPAIR_ALARM</AlarmRepairBuildingTL>
        <AlarmRepairMaterial>G_Wood</AlarmRepairMaterial>
        <AlarmRepairProvider>G_Wood_Supplier</AlarmRepairProvider>
        <MaxFloorFireSize>0</MaxFloorFireSize>
        <Cost>
            <GoodAmount>
                <GoodType>G_Wood</GoodType>
                <Amount>1</Amount>
            </GoodAmount>
            <!--GoodAmount>
            <GoodType>G_Wood</GoodType>
            <Amount>10</Amount>
        </GoodAmount-->
    </Cost>
    <BlockingAreas>
            <BlockingArea>
                <Blocked1>
                    <X>0</X>
                    <Y>0</Y>
                </Blocked1>
                <Blocked2>
                    <X>0</X>
                    <Y>0</Y>
                </Blocked2>
            </BlockingArea>
        </BlockingAreas>
        <BuildBlockingAreas>
            <BuildBlockingArea>
                <Blocked1>
                    <X>-170</X>
                    <Y>-80</Y>
                </Blocked1>
                <Blocked2>
                    <X>170</X>
                    <Y>80</Y>
                </Blocked2>
            </BuildBlockingArea>            
        </BuildBlockingAreas>
    <GateAreas>
        <GateArea>
            <Blocked1>
                <X>-170</X>
                <Y>-80</Y>
            </Blocked1>
            <Blocked2>
                <X>170</X>
                <Y>80</Y>
            </Blocked2>
        </GateArea>
    </GateAreas>

    <ConstructionInfo>
        <Time>5</Time>

        <BuilderSlots>
            <BuilderSlot>
                <Position>
                    <X>100</X>
                    <Y>-200</Y>
                </Position>
                <Orientation>0</Orientation>
            </BuilderSlot>
        </BuilderSlots>
        <ConstructionSite>B_BuildingPlot_PalisadeGate</ConstructionSite>
    </ConstructionInfo>

</Logic>

<Behaviors>
    <Behavior>
        <Logic classname="EGL::CBehaviorProps">
            <Class>EGL::CBehaviorMultiSubAnims</Class>
        </Logic>
        <Display classname="ED::CBehaviorProps">
            <Class>ED::CDisplayBehaviorBuildingAnimation</Class>
        </Display>
    </Behavior>
</Behaviors>

<MapEditor>
    <TreePosition>constructions\camps</TreePosition>
    <ShowInTree>false</ShowInTree>
    <Clime>Generic</Clime>
    <Scalable>false</Scalable>
    <TemplateResistant>true</TemplateResistant>
</MapEditor>
</root>
