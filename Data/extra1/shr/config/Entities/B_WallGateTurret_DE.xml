<?xml version="1.0" encoding="utf-8" ?>
<root>
    <Display classname="ED::CBuildingProps">
        <Class>ED::CWallBuilding</Class>
        <Model>Buildings/B_WallGateTurret_ME</Model>
        <Anims></Anims>
        <ConstructionSite>Buildings/B_WallTurret_BuildingSite_ME</ConstructionSite>
    </Display>

    <Logic classname="EGL::CWallTurretProps">
        <Class>EGL::CWallTurret</Class>
        <AccessCategory>AccessCategoryBuilding</AccessCategory>
        <NeedsPlayer>true</NeedsPlayer>
        <EntityCategories>
            <EntityCategory>Wall</EntityCategory>
            <EntityCategory>Turret</EntityCategory>
        </EntityCategories>
        <MapFileDontSave>true</MapFileDontSave>
        <ForceNoPlayer>false</ForceNoPlayer>
        <Worker>U_WallConstructionWorker</Worker> <!-- construction only -->
        <MaxWorkers>0</MaxWorkers> <!-- used for construction only -->
        <CanBeKnockedDown>false</CanBeKnockedDown>
        <MaxHealth>6000</MaxHealth>
        <ResistsSplashDamage>true</ResistsSplashDamage>
        <Exploration>0</Exploration>
        <FlattenOnBuild>true</FlattenOnBuild>
        <MayRemoveEntities>true</MayRemoveEntities>
        <IsHeightLocking>true</IsHeightLocking>
        <CanBeBuildOnOuterRim>true</CanBeBuildOnOuterRim>
        <WallHeight>600</WallHeight>
        <IsMountable>true</IsMountable>
        <CanLeaveImmediately>true</CanLeaveImmediately>
        <MaxFloorFireSize>0</MaxFloorFireSize>
        <!-- replaced by higher prio -->
        <ReplacementPriority>30</ReplacementPriority>


        <ApproachPos>
            <X>0</X>
            <Y>400</Y>
        </ApproachPos>
        <ApproachR>0</ApproachR>
        <DoorPos>
            <X>0</X>
            <Y>0</Y>
        </DoorPos>
        <BlockingAreas>
            <BlockingArea>
                <Blocked1>
                    <X>-100</X>
                    <Y>-200</Y>
                </Blocked1>
                <Blocked2>
                    <X>100</X>
                    <Y>200</Y>
                </Blocked2>
            </BlockingArea>
            <BlockingArea>
                <Blocked1>
                    <X>-200</X>
                    <Y>-100</Y>
                </Blocked1>
                <Blocked2>
                    <X>200</X>
                    <Y>100</Y>
                </Blocked2>
            </BlockingArea>
        </BlockingAreas>
        <BuildBlockingAreas>
            <BuildBlockingArea>
                <Blocked1>
                    <X>-100</X>
                    <Y>-200</Y>
                </Blocked1>
                <Blocked2>
                    <X>100</X>
                    <Y>200</Y>
                </Blocked2>
            </BuildBlockingArea>
            <BuildBlockingArea>
                <Blocked1>
                    <X>-200</X>
                    <Y>-100</Y>
                </Blocked1>
                <Blocked2>
                    <X>200</X>
                    <Y>100</Y>
                </Blocked2>
            </BuildBlockingArea>
        </BuildBlockingAreas>

        <ConstructionInfo>
            <Time>15</Time>

            <BuilderSlots>
                <BuilderSlot>
                    <Position>
                        <X>100</X>
                        <Y>-200</Y>
                    </Position>
                    <Orientation>0</Orientation>
                </BuilderSlot>
            </BuilderSlots>
            <ConstructionSite>B_BuildingPlot_WallGate</ConstructionSite>
        </ConstructionInfo>

    </Logic>
    <Behaviors>
    </Behaviors>
    <MapEditor>
        <TreePosition>buildings\camps</TreePosition>
        <ShowInTree>false</ShowInTree>
        <Clime>DarkEvelance</Clime>
        <Scalable>false</Scalable>
        <TemplateResistant>true</TemplateResistant>
    </MapEditor>
</root>
