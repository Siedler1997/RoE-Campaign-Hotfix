        <MaxHealth>200</MaxHealth>
        <AccessCategory>AccessCategoryBuilding</AccessCategory>
		<NeedsPlayer>true</NeedsPlayer>
		<ForceNoPlayer>false</ForceNoPlayer>
		<CanBeKnockedDown>false</CanBeKnockedDown>
		<Worker>U_OutpostConstructionWorker</Worker>
		<MaxWorkers>0</MaxWorkers>
		<MaxHealth>250</MaxHealth>
        <NoAutoRepair>true</NoAutoRepair>
		<RepairBuildingTL>TL_WORKER_REPAIR</RepairBuildingTL>
		<MaxFloorFireSize>0</MaxFloorFireSize>

        <Exploration>0</Exploration>
		<EntityCategories>
			  <EntityCategory>AttackableBuilding</EntityCategory>
			  <EntityCategory>BanditsCamp</EntityCategory>
			  <EntityCategory>CityBuilding</EntityCategory>
		</EntityCategories>
        <IsHeightLocking>true</IsHeightLocking>
    
        <UpgradeCosts>
           <UpgradeCost>
               <GoodAmount>
                   <GoodType>G_Wood</GoodType>
                   <Amount>1</Amount>
               </GoodAmount>
               <GoodAmount>
                   <GoodType>G_Wood</GoodType>
                   <Amount>1</Amount>
               </GoodAmount>
           </UpgradeCost>
        
           <UpgradeCost>
               <GoodAmount>
                   <GoodType>G_Wood</GoodType>
                   <Amount>1</Amount>
               </GoodAmount>
               <GoodAmount>
                   <GoodType>G_Wood</GoodType>
                   <Amount>1</Amount>
               </GoodAmount>
            </UpgradeCost>
        </UpgradeCosts>
      <ApproachPos>
          <X>200</X>
          <Y>-500</Y>
        </ApproachPos>
        <ApproachR>0</ApproachR>
        <DoorPos>
          <X>100</X>
          <Y>-100</Y>
        </DoorPos>
       <BlockingAreas>
          <BlockingArea>
            <Blocked1>
              <X>-300</X>
              <Y>300</Y>
            </Blocked1>
            <Blocked2>
              <X>400</X>
              <Y>-400</Y>
            </Blocked2>
          </BlockingArea>
        </BlockingAreas>
        <BuildBlockingAreas>
          <BuildBlockingArea>
            <Blocked1>
              <X>-300</X>
              <Y>300</Y>
            </Blocked1>
            <Blocked2>
              <X>400</X>
              <Y>-550</Y>
            </Blocked2>
          </BuildBlockingArea>
