<?xml version="1.0" encoding="utf-8" standalone="no" ?><root>
    <Display classname="ED::CDisplaySettlerWithSlaveProps">
        <Class>ED::CDisplaySettlerWithSlave</Class>
        <DrawPlayerColor>True</DrawPlayerColor>
        <Model>Vehicles\V_Trebuchet</Model>
        <Model2>Vehicles\V_Trebuchet_Turntable</Model2>
        <SlaveAlwaysVisible>true</SlaveAlwaysVisible>
        <StaticSlave>true</StaticSlave>
    </Display>
    <Logic classname="EGL::CSettlerProps">
        <Class>EGL::CBigSettler</Class>
        <AccessCategory>AccessCategoryAttackableSettler</AccessCategory>
        <PlayerSectorType>PlayerSectorTypeMilitary</PlayerSectorType>
        <MaxHealth>150</MaxHealth>
        <HealingSeconds/>
        <HealingPoints>0</HealingPoints>

        <MapFileDontSave>true</MapFileDontSave>
        <NeedsPlayer>true</NeedsPlayer>
        <ForceNoPlayer>false</ForceNoPlayer>
        <CanBePlacedOnBlockedGround>true</CanBePlacedOnBlockedGround>
        <EntityCategories>
            <EntityCategory>Military</EntityCategory>
			<EntityCategory>HeavyWeapon</EntityCategory>
            <EntityCategory>Trebuchet</EntityCategory>
        </EntityCategories>
        <AdjustWalkAnimSpeed>true</AdjustWalkAnimSpeed>
        <Exploration>30</Exploration>
        <IdleTaskList>TL_MILITARY_DEFEND</IdleTaskList>
        <CanDrown>false</CanDrown>
        <IsSelectable>true</IsSelectable>

        <CanBePlacedOnBlockedGround>true</CanBePlacedOnBlockedGround>

        <Cost>
        </Cost>

        <Familiar>
            <With>SheepPasture</With>
            <With>CattlePasture</With>
        </Familiar>

<SoundImpactType>Wood</SoundImpactType>

		<OnScreenIcon>MinimapCatapult</OnScreenIcon>
		<ShowInMiniMap>0.75</ShowInMiniMap>
    </Logic>

    <Behaviors>

        <Behavior>
            <Logic classname="EGL::CMilitaryMovementBehaviorProps">
                <Class>EGL::CMilitaryMovementBehavior</Class>
                <Speed>0</Speed>
                <RotationSpeed>50</RotationSpeed>
                <MoveTaskList>TL_MILITARY_WALK</MoveTaskList>
                <MoveIdleAnim>V_Trebuchet_Stand</MoveIdleAnim>
            </Logic>
            <Display classname="ED::CBehaviorProps">
                <Class>ED::CMovementBehaviorSlaveStatic</Class>
            </Display>
        </Behavior>

        <Behavior>
            <Logic classname="EGL::CAnimationBehaviorExProps">
                <Class>EGL::CBehaviorAnimationEx</Class>

                <AnimSet>SET_MILITARYTREBUCHET</AnimSet>
            </Logic>

            <Display classname="ED::CBehaviorProps">
                <Class>ED::CDisplayBehaviorAnimation</Class>
            </Display>
        </Behavior>

        <Behavior>
            <Logic classname="EGL::CBehaviorPropsDying">
                <Class>EGL::CBehaviorDying</Class>
                <DyingTaskList>TL_DIE</DyingTaskList>
            </Logic>
        </Behavior>

        <Behavior>
            <Logic classname="EGL::CStationaryFightBehaviorProps">
                <Class>EGL::CStationaryFightBehavior</Class>
                <DefendTL>TL_MILITARY_DEFEND</DefendTL>
                <MoveTaskList>TL_KNIGHT_WALK</MoveTaskList>
                <DefendRange>0</DefendRange>
                <AttackMoveRange>0</AttackMoveRange>

                <AmmunitionType>G_CatapultAmmunition</AmmunitionType>
                <AmmunitionLimit>10</AmmunitionLimit>

               <TargetCategoryList>
				   <!-- Walls -->
                    <TargetCategory>
                        <DisableAutoAttack>true</DisableAutoAttack>

                        <EntityCategoryList>
                            <EntityCategory>CityWallGate</EntityCategory>
                            <EntityCategory>CityWallSegment</EntityCategory>
							<EntityCategory/>
							<EntityCategory/>
							<EntityCategory/>
                            <EntityCategory/>
                        </EntityCategoryList>
                        <AttackModeList>
                            <AttackMode>
                                <BattleTL>TL_BATTLE_CATAPULT</BattleTL>
                                <!-- if ApplyDamageModifiers is false, the damage isn't modified by City Reputation and TerritoryBonus -->
                                <ApplyDamageModifiers>false</ApplyDamageModifiers>

                                <MinRange>1000</MinRange>
                                <MaxRange>64000</MaxRange>
                                <ProjectileEffect>E_Trebuchet_Fire_Wall</ProjectileEffect>
                                <MaxHeightDeltaToTarget>2000</MaxHeightDeltaToTarget>
                                <CanAttackOverWalls>true</CanAttackOverWalls>
                                <CooldownThousandths>8000</CooldownThousandths>
                                <ConsumeAmmunition>true</ConsumeAmmunition>
                                <Damage>
                                    <DiceCount>0</DiceCount>
                                    <FaceCount>0</FaceCount>
                                    <BaseValue>250</BaseValue>
                                </Damage>
                                <BuildingFireSize>20</BuildingFireSize>
                                <DamageRadiusList>
                                    <!-- Must be sorted from inner to outer. -->
                                    <DamageRadius> <Radius>200</Radius> <Percentage>100</Percentage> </DamageRadius>
                                    <DamageRadius> <Radius>400</Radius> <Percentage> 50</Percentage> </DamageRadius>
                                    <DamageRadius> <Radius>600</Radius> <Percentage> 25</Percentage> </DamageRadius>
                                </DamageRadiusList>
                            </AttackMode>
                        </AttackModeList>
                    </TargetCategory>

				   <!-- Palisades -->
				   <TargetCategory>
					   <DisableAutoAttack>true</DisableAutoAttack>

					   <EntityCategoryList>
						   <EntityCategory>PalisadeSegment</EntityCategory>
						   <EntityCategory/>
						   <EntityCategory/>
						   <EntityCategory/>
						   <EntityCategory/>
						   <EntityCategory/>
					   </EntityCategoryList>
					   <AttackModeList>
						   <AttackMode>
							   <BattleTL>TL_BATTLE_CATAPULT</BattleTL>
							   <!-- if ApplyDamageModifiers is false, the damage isn't modified by City Reputation and TerritoryBonus -->
							   <ApplyDamageModifiers>false</ApplyDamageModifiers>

							   <MinRange>1000</MinRange>
							   <MaxRange>64000</MaxRange>
							   <ProjectileEffect>E_Trebuchet_Fire_Palisade</ProjectileEffect>
							   <MaxHeightDeltaToTarget>2000</MaxHeightDeltaToTarget>
							   <CanAttackOverWalls>true</CanAttackOverWalls>
							   <CooldownThousandths>8000</CooldownThousandths>
							   <ConsumeAmmunition>true</ConsumeAmmunition>
							   <Damage>
								   <DiceCount>0</DiceCount>
								   <FaceCount>0</FaceCount>
								   <BaseValue>250</BaseValue>
							   </Damage>
							   <BuildingFireSize>20</BuildingFireSize>
							   <DamageRadiusList>
								   <!-- Must be sorted from inner to outer. -->
								   <DamageRadius>
									   <Radius>200</Radius>
									   <Percentage>100</Percentage>
								   </DamageRadius>
								   <DamageRadius>
									   <Radius>400</Radius>
									   <Percentage> 50</Percentage>
								   </DamageRadius>
								   <DamageRadius>
									   <Radius>600</Radius>
									   <Percentage> 25</Percentage>
								   </DamageRadius>
							   </DamageRadiusList>
						   </AttackMode>
					   </AttackModeList>
				   </TargetCategory>

				   <!-- Dragon -->
				   <TargetCategory>
					   <DisableAutoAttack>true</DisableAutoAttack>

					   <EntityCategoryList>
						   <EntityCategory>Dragon</EntityCategory>
						   <EntityCategory/>
						   <EntityCategory/>
						   <EntityCategory/>
						   <EntityCategory/>
						   <EntityCategory/>
					   </EntityCategoryList>
					   <AttackModeList>
						   <AttackMode>
							   <BattleTL>TL_BATTLE_CATAPULT</BattleTL>
							   <!-- if ApplyDamageModifiers is false, the damage isn't modified by City Reputation and TerritoryBonus -->
							   <ApplyDamageModifiers>false</ApplyDamageModifiers>

							   <MinRange>1000</MinRange>
							   <MaxRange>64000</MaxRange>
							   <ProjectileEffect>E_Trebuchet_Fire_Fly</ProjectileEffect>
							   <MaxHeightDeltaToTarget>2000</MaxHeightDeltaToTarget>
							   <CanAttackOverWalls>true</CanAttackOverWalls>
							   <CooldownThousandths>8000</CooldownThousandths>
							   <ConsumeAmmunition>true</ConsumeAmmunition>
							   <Damage>
								   <DiceCount>0</DiceCount>
								   <FaceCount>0</FaceCount>
								   <BaseValue>250</BaseValue>
							   </Damage>
							   <BuildingFireSize>20</BuildingFireSize>
							   <DamageRadiusList>
								   <!-- Must be sorted from inner to outer. -->
								   <DamageRadius>
									   <Radius>200</Radius>
									   <Percentage>100</Percentage>
								   </DamageRadius>
								   <DamageRadius>
									   <Radius>400</Radius>
									   <Percentage> 50</Percentage>
								   </DamageRadius>
								   <DamageRadius>
									   <Radius>600</Radius>
									   <Percentage> 25</Percentage>
								   </DamageRadius>
							   </DamageRadiusList>
						   </AttackMode>
					   </AttackModeList>
				   </TargetCategory>

				   <!-- And the rest ... -->
				   <TargetCategory>
					   <DisableAutoAttack>true</DisableAutoAttack>

					   <EntityCategoryList>
						   <EntityCategory>AttackableBuilding</EntityCategory>
						   <EntityCategory>HeavyWeapon</EntityCategory>
						   <EntityCategory/>
						   <EntityCategory/>
						   <EntityCategory/>
						   <EntityCategory/>
					   </EntityCategoryList>
					   <AttackModeList>
						   <AttackMode>
							   <BattleTL>TL_BATTLE_CATAPULT</BattleTL>
							   <!-- if ApplyDamageModifiers is false, the damage isn't modified by City Reputation and TerritoryBonus -->
							   <ApplyDamageModifiers>false</ApplyDamageModifiers>

							   <MinRange>1000</MinRange>
							   <MaxRange>64000</MaxRange>
							   <ProjectileEffect>E_Trebuchet_Fire</ProjectileEffect>
							   <MaxHeightDeltaToTarget>2000</MaxHeightDeltaToTarget>
							   <CanAttackOverWalls>true</CanAttackOverWalls>
							   <CooldownThousandths>8000</CooldownThousandths>
							   <ConsumeAmmunition>true</ConsumeAmmunition>
							   <Damage>
								   <DiceCount>0</DiceCount>
								   <FaceCount>0</FaceCount>
								   <BaseValue>250</BaseValue>
							   </Damage>
							   <BuildingFireSize>20</BuildingFireSize>
							   <DamageRadiusList>
								   <!-- Must be sorted from inner to outer. -->
								   <DamageRadius>
									   <Radius>200</Radius>
									   <Percentage>100</Percentage>
								   </DamageRadius>
								   <DamageRadius>
									   <Radius>400</Radius>
									   <Percentage> 50</Percentage>
								   </DamageRadius>
								   <DamageRadius>
									   <Radius>600</Radius>
									   <Percentage> 25</Percentage>
								   </DamageRadius>
							   </DamageRadiusList>
						   </AttackMode>
					   </AttackModeList>
				   </TargetCategory>
                </TargetCategoryList>
            </Logic>
        </Behavior>
    </Behaviors>

    <MapEditor>
        <TreePosition>Units\Military\Heavy Weapons</TreePosition>
        <ShowInTree>false</ShowInTree>
        <Clime>Generic</Clime>
        <Scalable>false</Scalable>
    </MapEditor>
</root>
